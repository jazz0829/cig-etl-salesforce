  </salesforceUploadConfiguration>

  <connectionStrings>
    <add name="CustomerIntelligence" connectionString="Data Source=.;Initial Catalog=CustomerIntelligence;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>

